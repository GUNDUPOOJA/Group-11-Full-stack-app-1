
<% if (place_location && place_location.error) { %>
    <div class="alert alert-danger" role="alert"><%- JSON.stringify(place_location.error) %></div>
    <% } %>
  
  <div class="card">
    <div class="card-header">
      Add Location
    </div>
    <div class="card-body">
      <form action="/place_location/save" method="post">
        <div class="form-group">
          <label for="name">Name:</label>
          <input type="text" class="form-control" id="name" name="name" value="<%= place_location.name %>" autocomplete="off">
        </div>
        
        <br>
        <div class="form-group">
          <input type="submit" class="btn btn-info" title="Submits a POST /save request" value="Create" />
        </div>
      </form>
    </div>
  </div>